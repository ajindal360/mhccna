    <validationRules>
        <fullName>FiscalYrEndMarch</fullName>
        <active>true</active>
        <description>Fiscal Year Ends needs to to end on March 31, on a year later than the Fiscal Year Starts</description>
        <errorConditionFormula>NOT
(
AND
(
MONTH(Fiscal_Year_Ends__c) = 3,
DAY(Fiscal_Year_Ends__c) = 31,
YEAR(Fiscal_Year_Ends__c) = YEAR(Fiscal_Year_Starts__c) + 1
)
)</errorConditionFormula>
        <errorDisplayField>Fiscal_Year_Ends__c</errorDisplayField>
        <errorMessage>Your Fiscal Year needs to end on March 31 on the year following the start</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FiscalYrStartApril</fullName>
        <active>true</active>
        <description>Fiscal Year Starts, needs to start on April 1 of any year</description>
        <errorConditionFormula>NOT
(
AND
(
MONTH(Fiscal_Year_Starts__c) = 4,
DAY(Fiscal_Year_Starts__c) = 1
)
)</errorConditionFormula>
        <errorDisplayField>Fiscal_Year_Starts__c</errorDisplayField>
        <errorMessage>Fiscal Year needs to start on April 1st</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FiscalYrEndsIsLaterThanFiscalYrStart</fullName>
        <active>true</active>
        <description>Fiscal Year Ends should be later than Fiscal Year Starts</description>
        <errorConditionFormula>Fiscal_Year_Starts__c &gt; Fiscal_Year_Ends__c</errorConditionFormula>
        <errorDisplayField>Fiscal_Year_Starts__c</errorDisplayField>
        <errorMessage>The Fiscal Year needs to end after the Fiscal Year Start</errorMessage>
    </validationRules>
