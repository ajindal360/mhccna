    <fields>
        <fullName>Current_Montly_Target__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c), April__c,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 1, May__c ,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 2, June__c ,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 3, July__c ,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 4, August__c,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 5,September__c,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 6,October__c,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 7,November__c,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 8,December__c,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 9,January__c,
IF(Month(TODAY()) = Month(Fiscal_Year_Starts__c) + 10,February__c,March__c)))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Current Montly Target</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Current_Quarterly_Target__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(Month(TODAY()) &gt;= Month(Fiscal_Year_Starts__c),Month(TODAY()) &lt;= Month(Fiscal_Year_Starts__c) + 2), Q1__c,
IF(AND(Month(TODAY()) &gt;= Month(Fiscal_Year_Starts__c) + 3,Month(TODAY()) &lt;= Month(Fiscal_Year_Starts__c) + 5), Q2__c,
IF(AND(Month(TODAY()) &gt;= Month(Fiscal_Year_Starts__c) + 6,Month(TODAY()) &lt;= Month(Fiscal_Year_Starts__c) + 8), Q3__c, Q4__c)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Current Quarterly Target</label>
